<!--begin::Form-->
<%= form_with(model: color_asset, local: true, class: "m-form m-form--fit m-form--label-align-right m-form--group-seperator-dashed") do |form| %>

  <%= render 'layouts/myform/error_myform', object: color_asset %>

  <div class="form-group m-form__group row">
    <%= form.label :color_asset_name, "color Name", class: "col-lg-2 col-form-label" %>
    <div class="col-lg-3">
      <%= form.text_field :color_asset_name, class: "form-control m-input", placeholder: "color Name", required: true %>
      <span class="m-form__help">Please enter color Name</span>
    </div>
  </div>


  <%= render 'layouts/myform/submit_myform', form: form, cancelpath: color_assets_path %>

<% end %>
<!--end::Form-->