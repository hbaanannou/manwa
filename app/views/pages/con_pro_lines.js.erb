<% if params[:con_chart_display] == 'con_details' %>
concrete_high_line( '#line_concrete' , '/pages/con_pro_lines.json?start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'main')
$("#con_header").html("<%=j render 'con_header' %>")
<% elsif params[:con_chart_display] == 'con_summary' %>
concrete_high_line_one( '#line_concrete' , '/pages/con_pro_lines.json?start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#con_header").html("<%=j render 'con_header' %>")
<% else %>
concrete_high_line_one( '#line_concrete' , '/pages/con_pro_lines.json?start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#con_header").html("<%=j render 'con_header' %>")
<% end %>