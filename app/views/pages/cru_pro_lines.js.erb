<% if params[:cru_chart_display] == 'cru_details' %>
crusher_high_line( '#line_crusher' , '/pages/cru_pro_lines.json?start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'main')
$("#cru_header").html("<%=j render 'cru_header' %>")
<% elsif params[:cru_chart_display] == 'cru_summary' %>
crusher_high_line_one( '#line_crusher' , '/pages/cru_pro_lines.json?start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#cru_header").html("<%=j render 'cru_header' %>")
<% else %>
crusher_high_line_one( '#line_crusher' , '/pages/cru_pro_lines.json?start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#cru_header").html("<%=j render 'cru_header' %>")
<% end %>