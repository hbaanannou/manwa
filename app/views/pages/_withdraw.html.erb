				<div class="m-widget5">
				
				<% if @withdraw_assets.by_delivery_site_user_0(current_user.id).count > 0 %>
					<% @withdraw_assets.by_delivery_site_user_0(current_user.id).each do |withdraw_asset| %>
						<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: true  %>
					<% end %>
				<% else %>
					<p class="text-center"> - No Opened Requestes now - </p>
				<% end %>

				</div>
			</div>
			<% end %>


			<% if current_user.role?(:assets_manager) %>
			<div class="tab-pane" id="withdraw_tab4_content" aria-expanded="false">

				<div class="m-widget5">
					<% if @withdraw_assets.posted.count > 0 %>
					<% @withdraw_assets.posted.each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: false  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Posted Requestes now - </p>
				<% end %>
					
				</div>

			</div>
		<% end %>


		<% if current_user.role?(:assets_province) %>
			<div class="tab-pane" id="withdraw_tab5_content" aria-expanded="false">

				<div class="m-widget5">
					<% if @withdraw_assets.by_delivery_province_user_1(current_user.id).count > 0 %>
					<% @withdraw_assets.by_delivery_province_user_1(current_user.id).each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: false  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Posted Requestes now - </p>
				<% end %>
					
				</div>

			</div>


			<div class="tab-pane" id="withdraw_tab6_content" aria-expanded="false">

				<div class="m-widget5">
					<% if @withdraw_assets.by_receive_province_user_1(current_user.id).count > 0 %>
					<% @withdraw_assets.by_receive_province_user_1(current_user.id).each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: false  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Posted Requestes now - </p>
				<% end %>
					
				</div>

			</div>
		<% end %>


		<% if current_user.role?(:assets_site) %>
			<div class="tab-pane" id="withdraw_tab7_content" aria-expanded="false">

				<div class="m-widget5">
					<% if @withdraw_assets.by_delivery_site_user_1(current_user.id).count > 0 %>
					<% @withdraw_assets.by_delivery_site_user_1(current_user.id).each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: false  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Posted Requestes now - </p>
				<% end %>
					
				</div>

			</div>


			<div class="tab-pane" id="withdraw_tab8_content" aria-expanded="false">

				<div class="m-widget5">
					<% if @withdraw_assets.by_receive_site_user_1(current_user.id).count > 0 %>
					<% @withdraw_assets.by_receive_site_user_1(current_user.id).each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: true  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Posted Requestes now - </p>
				<% end %>
					
				</div>

			</div>
		<% end %>


		<% if current_user.role?(:assets_manager) %>
			<div class="tab-pane" id="withdraw_tab9_content" aria-expanded="false">

				<p class="text-center"> - Last 5 Received transfer - </p>

				<div class="m-widget5">
					<% if @withdraw_assets.received.count > 0 %>
					<% @withdraw_assets.received.limit(5).each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: false  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Received Requestes now - </p>
				<% end %>
					
				</div>

			</div>
		<% end %>


		<% if current_user.role?(:assets_province) %>

		<div class="tab-pane" id="withdraw_tab10_content" aria-expanded="false">

				<p class="text-center"> - Last 5 Delivered withdraw - </p>

				<div class="m-widget5">
					<% if @withdraw_assets.by_delivery_province_user_2(current_user.id).count > 0 %>
					<% @withdraw_assets.by_delivery_province_user_2(current_user.id).limit(5).each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: 'Withdrawn', can_edit: false  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Withdrawn Requestes now - </p>
				<% end %>
					
				</div>

			</div>


			<div class="tab-pane" id="withdraw_tab11_content" aria-expanded="false">

				<p class="text-center"> - Last 5 Received withdraw - </p>

				<div class="m-widget5">
