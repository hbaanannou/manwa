<div class="m-portlet m-portlet--full-height ">
	<div class="m-portlet__head">
		<div class="m-portlet__head-caption">
			<div class="m-portlet__head-title">
				<span class="m-portlet__head-icon">
        <i class="flaticon-delete m--font-brand"></i>
      	</span>
				<h3 class="m-portlet__head-text">
					Withdraw Requests
				</h3>
				<span class="m-portlet__head-icon pl-2">
        	<%= link_to home_pages_path(p_name: 'withdraw'), remote: true do %>
	        	<i class="flaticon-refresh m--font-danger"></i>
	        <% end %>
      	</span>
			</div>
		</div>
		<div class="m-portlet__head-tools">
			<ul class="nav nav-pills nav-pills--brand m-nav-pills--align-right m-nav-pills--btn-pill m-nav-pills--btn-sm" role="tablist">

				<% if current_user.role?(:assets_manager) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link active" data-toggle="tab" href="#withdraw_tab1_content" role="tab">
						Opened &nbsp;
						<% if @withdraw_assets.opened.count > 0 %>
						<span class="m-badge m-badge--default"><%= @withdraw_assets.opened.count %></span>
						<% end %>
					</a>
				</li>
				<% end %>
				<% if current_user.role?(:assets_province) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link active" data-toggle="tab" href="#withdraw_tab2_content" role="tab">
						Opened &nbsp;
						<% if @withdraw_assets.by_delivery_province_user_0(current_user.id).count > 0 %>
						<span class="m-badge m-badge--danger"><%= @withdraw_assets.by_delivery_province_user_0(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<% end %>
				<% if current_user.role?(:assets_site) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link active" data-toggle="tab" href="#withdraw_tab3_content" role="tab">
						Opened &nbsp;
						<% if @withdraw_assets.by_delivery_site_user_0(current_user.id).count > 0 %>
						<span class="m-badge m-badge--danger"><%= @withdraw_assets.by_delivery_site_user_0(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<% end %>



				<% if current_user.role?(:assets_manager) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link" data-toggle="tab" href="#withdraw_tab4_content" role="tab">
						Posted &nbsp;
						<% if @withdraw_assets.posted.count > 0 %>
						<span class="m-badge m-badge--danger"><%= @withdraw_assets.posted.count %></span>
						<% end %>
					</a>
				</li>
				<% end %>
				<% if current_user.role?(:assets_province) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link" data-toggle="tab" href="#withdraw_tab5_content" role="tab">
						Posted From &nbsp;
						<% if @withdraw_assets.by_delivery_province_user_1(current_user.id).count > 0 %>
						<span class="m-badge m-badge--danger"><%= @withdraw_assets.by_delivery_province_user_1(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link" data-toggle="tab" href="#withdraw_tab6_content" role="tab">
						Posted To &nbsp;
						<% if @withdraw_assets.by_receive_province_user_1(current_user.id).count > 0 %>
						<span class="m-badge m-badge--success"><%= @withdraw_assets.by_receive_province_user_1(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<% end %>
				<% if current_user.role?(:assets_site) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link" data-toggle="tab" href="#withdraw_tab7_content" role="tab">
						Posted From &nbsp;
						<% if @withdraw_assets.by_delivery_site_user_1(current_user.id).count > 0 %>
						<span class="m-badge m-badge--danger"><%= @withdraw_assets.by_delivery_site_user_1(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link" data-toggle="tab" href="#withdraw_tab8_content" role="tab">
						Posted To &nbsp;
						<% if @withdraw_assets.by_receive_site_user_1(current_user.id).count > 0 %>
						<span class="m-badge m-badge--success"><%= @withdraw_assets.by_receive_site_user_1(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<% end %>


				<% if current_user.role?(:assets_manager) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link " data-toggle="tab" href="#withdraw_tab9_content" role="tab">
						Received &nbsp;
						<% if @withdraw_assets.received.count > 0 %>
						<span class="m-badge m-badge--success"><%= @withdraw_assets.received.count %></span>
						<% end %>
					</a>
				</li>
				<% end %>
				<% if current_user.role?(:assets_province) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link " data-toggle="tab" href="#withdraw_tab10_content" role="tab">
						Delivered &nbsp;
						<% if @withdraw_assets.by_delivery_province_user_2(current_user.id).count > 0 %>
						<span class="m-badge m-badge--default"><%= @withdraw_assets.by_delivery_province_user_2(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link " data-toggle="tab" href="#withdraw_tab11_content" role="tab">
						Received &nbsp;
						<% if @withdraw_assets.by_receive_province_user_2(current_user.id).count > 0 %>
						<span class="m-badge m-badge--success"><%= @withdraw_assets.by_receive_province_user_2(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<% end %>
				<% if current_user.role?(:assets_site) %>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link " data-toggle="tab" href="#withdraw_tab12_content" role="tab">
						Delivered &nbsp;
						<% if @withdraw_assets.by_delivery_site_user_2(current_user.id).count > 0 %>
						<span class="m-badge m-badge--default"><%= @withdraw_assets.by_delivery_site_user_2(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<li class="nav-item m-tabs__item">
					<a class="nav-link m-tabs__link " data-toggle="tab" href="#withdraw_tab13_content" role="tab">
						Received &nbsp;
						<% if @withdraw_assets.by_receive_site_user_2(current_user.id).count > 0 %>
						<span class="m-badge m-badge--success"><%= @withdraw_assets.by_receive_site_user_2(current_user.id).count %></span>
						<% end %>
					</a>
				</li>
				<% end %>


			</ul>
		</div>
	</div>
	<div class="m-portlet__body">
		<!--begin::Content-->
		<div class="tab-content">

			<% if current_user.role?(:assets_manager) %>
			<div class="tab-pane active" id="withdraw_tab1_content" aria-expanded="true">
				<div class="m-widget5">
				
					<% if @withdraw_assets.opened.count > 0 %>
						<% @withdraw_assets.opened.each do |withdraw_asset| %>
							<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: false  %>
						<% end %>
					<% else %>
					<p class="text-center"> - No Opened Requestes now - </p>
				<% end %>

				</div>
			</div>
			<% end %>
			<% if current_user.role?(:assets_province) %>
			<div class="tab-pane active" id="withdraw_tab2_content" aria-expanded="true">
				<div class="m-widget5">
				
				<% if @withdraw_assets.by_delivery_province_user_0(current_user.id).count > 0 %>
