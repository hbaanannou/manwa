				</div>

			</div>


			<div class="tab-pane" id="withdraw_tab13_content" aria-expanded="false">

				<p class="text-center"> - Last 5 Received withdraw - </p>

				<div class="m-widget5">
					<% if @withdraw_assets.by_receive_site_user_2(current_user.id).count > 0 %>
					<% @withdraw_assets.by_receive_site_user_2(current_user.id).limit(5).each do |withdraw_asset| %>

					<%= render 'item_withdraw', withdraw_asset: withdraw_asset, withdraw_status: withdraw_asset.withdraw_status, can_edit: false  %>

				<% end %>
				<% else %>
					<p class="text-center"> - No Received Requestes now - </p>
				<% end %>
					
				</div>

			</div>
		<% end %>



		</div>
		<!--end::Content-->
	</div>
</div>