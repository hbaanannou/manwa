<%= render 'layouts/myshow/before_myshow', mytitle: "Type" %>

<div class="m-widget13__item">
    <span class="m-widget13__desc m--align-right">
      Type Name
    </span>
  <span class="m-widget13__text m-widget13__text-bolder">
      <%= @type_asset.type_asset_name %>
    </span>
</div>

<div class="m-widget13__item">
    <span class="m-widget13__desc m--align-right">
      Model Names
    </span>
  <span class="m-widget13__text m-widget13__text-bolder">
      <% @type_asset.model_asset_ids.map do |r| %>
        <%= "#{ModelAsset.find(r).model_asset_name}, " %>
      <% end %>
    </span>
</div>

<%= render 'layouts/myshow/after_myshow', editpath: edit_type_asset_path(@type_asset), backpath: type_assets_path %>