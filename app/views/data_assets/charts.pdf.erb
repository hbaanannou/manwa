

<!--Begin::Section-->
<div class="m-section">
  <div class="m-section__content">
    <div class="table-responsive">

    <table class="table m-table m-table--border-success m-table--head-bg-success table-bordered table-hover text-center">
      <thead>
        <tr>
          <th>Hours Sum</th>
          <th>Hours Average</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td class="m--font-info font-weight-bold" id="checklists_hours_sum_id"><%= @pdf_checklists_hours_sum %></td>
          <td class="m--font-brand font-weight-bold" id="checklists_hours_average_id"><%= @pdf_checklists_hours_average %></td>
        </tr>
      </tbody>
    </table>


    <table class="table m-table m-table--border-success m-table--head-bg-success table-bordered table-hover text-center">
      <thead>
        <tr>
          <th>Meters Different</th>
          <th>Meters Average</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td class="m--font-info font-weight-bold" id="checklists_meters_different_id">
            <% if @pdf_checklists_meters_end_date %>
            <%= @pdf_checklists_meters_end_date - @pdf_checklists_meters_start_date %>
            <% end %>
            </td>
          <td class="m--font-brand font-weight-bold" id="checklists_meters_average_id">
            <% if @pdf_checklists_meters_end_date %>
            <%= ((@pdf_checklists_meters_end_date - @pdf_checklists_meters_start_date) / @pdf_checklists_meters_count.to_f)  %>
            <% end %>
            </td>
        </tr>
      </tbody>
    </table>



