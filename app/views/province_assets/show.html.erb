<%= render 'layouts/myshow/before_myshow', mytitle: "Province" %>

<div class="m-widget13__item">
    <span class="m-widget13__desc m--align-right">
      Province Name
    </span>
  <span class="m-widget13__text m-widget13__text-bolder">
      <%= @province_asset.province_asset_name %>
    </span>
</div>

<div class="m-widget13__item">
    <span class="m-widget13__desc m--align-right">
      Area Name
    </span>
  <span class="m-widget13__text m-widget13__text-bolder">
      <%= @province_asset.area_asset.area_asset_name %>
    </span>
</div>

<div class="m-widget13__item">
    <span class="m-widget13__desc m--align-right">
      Province Manager
    </span>
  <span class="m-widget13__text m-widget13__text-bolder">
      <%= @province_asset.user.email %>
    </span>
</div>

<%= render 'layouts/myshow/after_myshow', editpath: edit_province_asset_path(@province_asset), backpath: province_assets_path %>