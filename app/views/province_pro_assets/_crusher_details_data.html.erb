<!--begin:: Widgets/Authors Profit-->
<div class="m-portlet m-portlet--bordered-semi m-portlet--full-height ">
  <div class="m-portlet__head">
    <div class="m-portlet__head-caption">
      <div class="m-portlet__head-title">
        <h3 class="m-portlet__head-text">
          <strong>Crusher Details:</strong>
        </h3>
        
      </div>
    </div>
    <div class="m-portlet__head-tools">
      <ul class="m-portlet__nav">

        <li class="m-portlet__nav-item">
          <%= link_to cru_province_pro_details_province_pro_assets_path(province_id: params[:province_id], q: request.parameters[:q],format: :pdf), class: "btn m-btn m-btn--icon m-btn--pill m-btn--air btn-danger", target: "_blank" do %>
            <span>
              <i class="la la-file"></i>
              <span>
                Create PDF document
              </span>
            </span>
          <% end %>
        </li>

      </ul>
    </div>
  </div>
  <div class="m-portlet__body">


    <!--Begin::Section-->
    <div class="m-section">
      <div class="m-section__content">
        <div class="table-responsive">

        <table class="table m-table m-table--border-success m-table--head-bg-success table-bordered table-hover text-center">
          <thead>
            <tr>
              <th>Crusher Work Hours Sum</th>
              <th>Crusher Amount Sum (Meters)</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td class="m--font-info font-weight-bold" id="crusher_hours_sum_id"></td>
              <td class="m--font-brand font-weight-bold" id="crusher_amount_sum_id"></td>
            </tr>
          </tbody>
        </table>




        <table class="table m-table m-table--border-success m-table--head-bg-success table-bordered table-hover text-center">
          <thead>
            <tr>
              <th>Type - Product</th>
              <th>Amount (Meters)</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td class="m--font-default"><strong>3/8 IN</strong></td>
              <td class="font-weight-bold" id="crusher_3_8_in_id"></td>
            </tr>
            <tr>
              <td class="m--font-default"><strong>3/4 IN</strong></td>
              <td class="font-weight-bold" id="crusher_3_4_in_id"></td>
            </tr>
            <tr>
              <td class="m--font-default"><strong>1 IN</strong></td>
              <td class="font-weight-bold" id="crusher_1_in_id"></td>
            </tr>
            <tr>
              <td class="m--font-default"><strong>11/2 IN</strong></td>
              <td class="font-weight-bold" id="crusher_11_2_in_id"></td>
            </tr>
            <tr>
              <td class="m--font-default"><strong>Oversize</strong></td>
              <td class="font-weight-bold" id="crusher_oversize_id"></td>
            </tr>
            <tr>
              <td class="m--font-default"><strong>Aggregate</strong></td>
              <td class="font-weight-bold" id="crusher_aggregate_id"></td>
            </tr>

          </tbody>
        </table>


        </div>
      </div>
    </div>
    <!--End::Section-->


  </div>

</div>