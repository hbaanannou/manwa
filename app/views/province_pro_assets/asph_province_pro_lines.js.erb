<% if params[:asph_chart_display] == 'asph_details' %>
asphalt_high_line( '#line_asphalt' , '/province_pro_assets/asph_province_pro_lines.json?province_id=<%= params[:province_id] %>&start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'province')
$("#asph_header").html("<%=j render 'asph_header' %>")
<% elsif params[:asph_chart_display] == 'asph_summary' %>
asphalt_high_line_one( '#line_asphalt' , '/province_pro_assets/asph_province_pro_lines.json?province_id=<%= params[:province_id] %>&start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#asph_header").html("<%=j render 'asph_header' %>")
<% else %>
asphalt_high_line_one( '#line_asphalt' , '/province_pro_assets/asph_province_pro_lines.json?province_id=<%= params[:province_id] %>&start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#asph_header").html("<%=j render 'asph_header' %>")
<% end %>