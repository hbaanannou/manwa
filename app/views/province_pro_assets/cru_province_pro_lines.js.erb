<% if params[:cru_chart_display] == 'cru_details' %>
crusher_high_line( '#line_crusher' , '/province_pro_assets/cru_province_pro_lines.json?province_id=<%= params[:province_id] %>&start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'province')
$("#cru_header").html("<%=j render 'cru_header' %>")
<% elsif params[:cru_chart_display] == 'cru_summary' %>
crusher_high_line_one( '#line_crusher' , '/province_pro_assets/cru_province_pro_lines.json?province_id=<%= params[:province_id] %>&start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#cru_header").html("<%=j render 'cru_header' %>")
<% else %>
crusher_high_line_one( '#line_crusher' , '/province_pro_assets/cru_province_pro_lines.json?province_id=<%= params[:province_id] %>&start_date=<%= session[:start_date] if session[:start_date].present? %>&end_date=<%= session[:end_date] if session[:end_date].present? %>', 'one_line')
$("#cru_header").html("<%=j render 'cru_header' %>")
<% end %>